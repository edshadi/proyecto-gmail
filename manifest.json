{
  "name": "Proyecto Gmail",
  "version": "0.1",
  "description": "Project Mangement right in your gmail",
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*", "http://mail.google.com/*"],
      "js": ["lib/gmailr/bootstrap.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "http://mail.google.com/*",
    "https://mail.google.com/",
    "http://*/"
  ],
  "manifest_version" : 2,
  "web_accessible_resources" : [ "lib/gmailr/bootstrap.js", "lib/gmailr/gmailr.js", "lib/gmailr/init.js", "lib/jquery.1.4.2.js", "lib/jquery.ba-bbq.js", "lib/lab.js", "main.css", "main.js"]
}
